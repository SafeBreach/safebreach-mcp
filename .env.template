# SafeBreach MCP Environment Variables Template
# Copy this file to .env and fill in your actual values

# SafeBreach API Tokens (DO NOT commit real tokens!)
CONSOLE_A_APITOKEN=your-console-a-token-here
CONSOLE_B_APITOKEN=your-console-b-token-here
CONSOLE_C_APITOKEN=your-console-c-token-here
CONSOLE_D_APITOKEN=your-console-d-token-here

# MCP Server Configuration
SAFEBREACH_MCP_AUTH_TOKEN=your-mcp-auth-token-here
SAFEBREACH_ENVS_FILE=path/to/your/environments.json

# External Access (optional)
SAFEBREACH_MCP_ALLOW_EXTERNAL=false
SAFEBREACH_MCP_BIND_HOST=127.0.0.1

# AWS Configuration (if using AWS secrets)
AWS_REGION=us-east-1
AWS_PROFILE=default